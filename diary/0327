#include <iostream> 
using namespace std;
int main() 
{ 
for(int i=1;i<=9;i++) 
{ 
for(int j=1;j<=9;j++) 
{ 
cout <<"%dX%d=%d\t",i,j,i*j<< endl; 
} 
cout <<"\n"<< endl; 
} 
system("PAUSE"); 
return 0; 
}
=================================================
