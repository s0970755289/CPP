                    C++
=================================================
#include<stdio.h>
int main()
{
    float h,s;
    h=s=100;
    h=h/2; //第一次反彈高度
    for(int i=2;i<=10;i++)
    {
        s=s+2*h;
        h=h/2;
    }
    printf("第10次落地時，共經過%f米，第10次反彈高%f米\n",s,h);
    return 0;
}
=================================================
                     C
=================================================
#include<iostream>
using namespace std;
int main()
{
    float h,s;
    h=s=100;
    h=h/2; //第一次反彈高度
    for(int i=2;i<=10;i++)
    {
        s=s+2*h;
        h=h/2;
    }
    cout <<"第10次落地時，共經過%f米，第10次反彈高%f米\n",s,h<< endl;
    return 0;
}
