mport java.lang.*;

public class Java
{
  public static void main(String[] args)                                         
  {
   for(int i = 1 ; i< 10 ; i++){
      for(int j = 1 ; j <10 ; j++)
         System.out.print(""+i+"*"+j+"="+(i*j)+"\t");
      System.out.println();
   }
  }
}

=================================================
                    (4)
                    C++
=================================================
#include<iostream>
using namespace std;
int main()
{
    int i,x,y,z;
    for(i=100;i<1000;i++)
    {
        x=i%10;
        y=i/10%10;
        z=i/100%10;
        
        if(i==(x*x*x+y*y*y+z*z*z))
        cout <<"%d\n",i<< endl;
        
    }
    return 0;
}
