public class JAVA {
    
    public static void main(String args[]){
        double s=0;
        double t=100;
        for(int i=1;i<=10;i++){
            s+=t;
            t=t/2;
        }
        System.out.println(s);
        System.out.println(t);
    } 
}
=================================================
                    (6)
                    C++
=================================================
#include <iostream>
using namespace std;
int main()
{
    char i,j,k;
    for(i='x';i<='z';i++) {
        for(j='x';j<='z';j++) {
            if(i!=j) {
                for(k='x';k<='z';k++) {
                    if(i!=k&&j!=k) {
                        if(i!='x'&&k!='x'&&k!='z') {
                            cout <<"顺序为：a--%c\tb--%c\tc--%c\n",i,j,k<< endl;
                        }
                    }
                }
            }
        }
    }
}
=================================================
                     C
=================================================
#include <stdio.h>
#include <stdlib.h>
 
int main()
{
    char i,j,k;
    for(i='x';i<='z';i++) {
        for(j='x';j<='z';j++) {
            if(i!=j) {
                for(k='x';k<='z';k++) {
                    if(i!=k&&j!=k) {
                        if(i!='x'&&k!='x'&&k!='z') {
                            printf("順序為：a--%c\tb--%c\tc--%c\n",i,j,k);
                        }
                    }
                }
            }
        }
    }
}
=================================================
                    JAVA
=================================================
public class Competitor {

    public static void main(String args[]) {
        Competitor c = new Competitor();
        c.competitor();
    }
    
    public void competitor(){
        
        String[] s={"x","y","z"};
        
        for(int i=0;i<s.length;i++){
            for(int j=0;j<s.length;j++){
                for(int k=0;k<s.length;k++){
                    String a=s[i];
                    String b=s[j];
                    String c=s[k];
                    if((!a.equals(b))&&(!b.equals(c))&&(!a.equals(c))&(!"x".equals(a))&&(!"x".equals(c))&&(!"y".equals(c))){
                        System.out.println("比賽名單如下：");
                        System.out.println("a  VS  "+a);
                        System.out.println("b  VS  "+b);
                        System.out.println("c  VS  "+c);
                    }
                }
            }
        }
        
    }
}
